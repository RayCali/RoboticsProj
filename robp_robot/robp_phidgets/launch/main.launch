<launch>
<include file="$(find robp_phidgets)/launch/software.launch"></include>
<include file="$(find robp_phidgets)/launch/hardware.launch"></include>
<include file="$(find realsense2_camera)/launch/rs_aligned_depth.launch">
    <!-- <arg name="filters" value="pointcloud"/> -->
    <arg name="align_depth" value="True"/>
</include>

<node pkg="tf2_ros" type="static_transform_publisher" name="arm_base_publisher" args="-0.085 -0.05 0.059 0 0 0 1 base_link arm_base" />
<node pkg="tf2_ros" type="static_transform_publisher" name="arm_cam_publisher" args="-0.075 0.0 0.042 0 0 0 1 end_effector arm_cam" />

</launch>

